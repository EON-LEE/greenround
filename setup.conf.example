# ==========================================
# Greenround GCP 환경 설정 파일
# ==========================================
# 이 내용을 프로젝트에 맞게 수정한 후 'setup.conf'로 복사하여 사용하세요.
# (cp setup.conf.example setup.conf)

# --- 프로젝트 설정 ---
# 새 GCP 프로젝트를 생성할지 여부 (true/false)
# true로 설정하면 아래 PROJECT_ID는 무시되고 랜덤 ID로 생성됩니다.
CREATE_NEW_PROJECT=true

# 기존 GCP 프로젝트 ID (CREATE_NEW_PROJECT=false 일 때만 사용)
# PROJECT_ID="your-existing-project-id"

# --- 리소스 설정 ---
# GCP 리전 (예: asia-northeast3, us-central1)
REGION="asia-northeast3"

# --- GitHub 연동 설정 ---
# Cloud Build 앱과 연동된 GitHub 저장소 이름
# (예: "your-github-username/your-repo-name")
GITHUB_REPO_NAME="EON-LEE/greenround"

# 서비스 이름의 접두사 (랜덤 서픽스는 스크립트에서 자동으로 추가됩니다)
SERVICE_NAME_PREFIX="greenround-backend"

# GCS 버킷 이름의 접두사 (랜덤 서픽스는 스크립트에서 자동으로 추가됩니다)
BUCKET_NAME_PREFIX="greenround-storage"

# Artifact Registry 저장소 이름 접두사
REPO_NAME_PREFIX="greenround"
# --- 사용자 추가 설정 ---
# 프로젝트에 추가할 사용자 이메일 목록 (공백으로 구분)
# 예: PROJECT_USERS=("user1@example.com" "user2@example.com")
PROJECT_USERS=(
    "everydaystudy@gmail.com"
    "entjqvv@gmail.com"
) 